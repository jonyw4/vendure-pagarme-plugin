  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cp:i18n": "ncp src/i18n lib/i18n",
    "cp": "npm run cp:i18n",
    "watch": "tsc --watch",
    "build": "rimraf lib && tsc && npm run cp",
    "lint": "eslint --ext .ts,.tsx",
    "lint:fix": "eslint --ext .ts,.tsx --fix",
    "test": "npm run test:e2e",
