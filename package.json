  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:jonyw4/vendure-pagarme-plugin.git",
  "author": "Jonathan CÃ©lio <jonathan.clio@hotmail.com>",
