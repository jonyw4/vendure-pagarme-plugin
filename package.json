    "semantic-release": "17.1.1",
    "ts-jest": "26.1.3",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  }
